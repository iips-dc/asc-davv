<div class="col-lg-12" id="filterrific_results">
        <div class="panel panel-default">
            <div class="panel-heading">
              <b>List Of Feedback</b>
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                      <thead>
                          <tr>
                            <th>S.no</th>
                            <th>Communication skill</th>
                            <th>Usefulness Of topic</th>
                            <th>Competency In subject</th>
                            <th>Quality Of lecture</th>
                            <th>Course</th>
                            <th>Resource person</th>
                          </tr>
                      </thead>  
                      <tbody>
                      <% i=0 %>  
                        <% @feedbacks.each do |feedback| %>
                          <tr>
                            <td><%= link_to i=i+1, feedback %></td>
                            <td><%= feedback.communication_skill %></td>
                            <td><%= feedback.usefulness_topic %></td>
                            <td><%= feedback.competency_subject %></td>
                            <td><%= feedback.quality_lecture %></td>
                            <td><%= feedback.course.course_name %></td>
                            <td>
                                <% if feedback.resource_person_id!= nil %> 
                                  <%= feedback.resource_person.person_name %>
                                <% else %>
                                  <%= N/A %>
                                <% end %>    
                            </td>
                          </tr>
                        <% end %>
                      </tbody>  
                    </table>
                    <%= paginate @feedbacks %>
                </div>
                <!-- /.table-responsive -->       
              </div>
              <!-- /.panel-body -->
            </div>
            <!-- /. panel -->    
</div>
<!-- /.col-lg-12 -->