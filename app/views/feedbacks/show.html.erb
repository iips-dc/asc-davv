<!-- row -->            
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Individual Record</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<!-- row -->
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <b>Record</b>
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table">
                      <thead>
                          <tr>
                            <th>Communication skill</th>
                            <th>Usefulness topic</th>
                            <th>Competency subject</th>
                            <th>Quality lecture</th>
                            <th>Course</th>
                            <th>Resource person</th>
                          </tr>
                      </thead>  
                      <tbody>
                        <tr class="info">
                          <td><%= @feedback.communication_skill %></td>
                          <td><%= @feedback.usefulness_topic %></td>
                          <td><%= @feedback.competency_subject %></td>
                          <td><%= @feedback.quality_lecture %></td>
                          <td><%= @feedback.course.course_name %></td>
                          <td><%= @feedback.resource_person.person_name %></td>
                        </tr>
                      </tbody> 
                  </table>
                </div>
                <!-- /.table-responsive -->       
              </div>
              <!-- /.panel-body -->
            </div>
            <!-- /. panel -->      
          <%= link_to 'Edit', edit_feedback_path(@feedback), :class => 'btn btn-default' %>
          <%= link_to 'Destroy', feedback_path, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn btn-default' %> 
          <%= link_to 'Back', feedbacks_path, :class => 'btn btn-default' %>          
        </div>
        <!-- /.col-lg-12 -->    
</div>
<!-- /. row -->




 



  



