<!-- row -->            
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Individual Record</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->

<div class="col-md-offset-1 col-md-10 col-md-offset-1">
    <%= render 'detail' %>     
    <%= link_to 'Edit', edit_interaction_program_path(@interaction_program), :class => 'btn btn-default' if can? :update, InteractionProgram %> 
    <%= link_to 'Delete', interaction_program_path, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn btn-default' if can? :destroy, InteractionProgram %>
    <%= link_to 'Back', interaction_record_path, :class => 'btn btn-default' %> 
    <%= link_to 'Download as Excel', interaction_program_path(format: "xlsx"), :class => 'btn btn-default' %>  
</div>
<!-- /. offset -->
