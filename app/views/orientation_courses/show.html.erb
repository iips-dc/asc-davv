<!-- row -->            
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Individual Record</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->

<div class="col-md-offset-1 col-md-10 col-md-offset-1">
    <%= render 'detail' %>

    <%= link_to 'Edit', edit_orientation_course_path(@orientation_course), :class => 'btn btn-default' if can? :update, OrientationCourse%>
    <%= link_to 'Delete', orientation_course_path, method: :delete, :class => 'btn btn-default', data: { confirm: 'Are you sure?' } if can? :destroy, OrientationCourse %> 
    <%= link_to 'Back', orientation_record_path, :class => 'btn btn-default' %>
    <%= link_to "Download as Excel", orientation_course_path(format: "xlsx"), :class => 'btn btn-default' %>
</div>
<!-- /. offset -->
