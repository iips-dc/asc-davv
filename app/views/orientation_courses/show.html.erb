<!-- row -->            
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Individual Record</h1>
    </div>
    <!-- /.col-lg-12 -->
    <p id="notice"><%= notice %></p>
</div>
<!-- /.row -->
<!-- row -->
<div class="row">
    <div class="col-lg-12">
          <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover">
                        <thead>          
                            <tr>
                                <th>Name</th>
                                <th>Gender</th>
                                <th>Religion</th>
                                <th>Category</th>
                                <th>Marital status</th>
                                <th>Email</th>
                                <th>Alternate Email</th>
                                <th>Stream</th>
                                <th>College</th>
                                <th>University</th>
                                <th>City</th>
                                <th>District</th>
                                <th>State</th>
                                <th>Position</th>
                                <th>Payscale</th>
                                <th>Course</th>
                                <th>Pin</th>
                                <th>Std Code</th>
                                <th>Landline</th>
                                <th>Mobile</th>
                                <th>University pin</th>
                                <th>Dob</th>
                                <th>Date of joining</th>
                                <th>Preferred date1</th>
                                <th>Preferred date2</th>
                                <th>Residential address</th>
                                <th>Phd</th>
                            </tr>
                        </thead>             
                        <tbody>
                          <tr>
                            <td><%= @orientation_course.name %></td>
                            <td><%= @orientation_course.gender %></td>
                            <td><%= @orientation_course.religion %></td>
                            <td><%= @orientation_course.category %></td>
                            <td><%= @orientation_course.marital_status %></td>
                            <td><%= @orientation_course.email %></td>
                            <td><%= @orientation_course.alternate_email %></td>
                            <td><%= @orientation_course.stream %></td>
                            <td><%= @orientation_course.college %></td>
                            <td><%= @orientation_course.university %></td>
                            <td><%= @orientation_course.city %></td>
                            <td><%= @orientation_course.district %></td>
                            <td><%= @orientation_course.state %></td>
                            <td><%= @orientation_course.position %></td>
                            <td><%= @orientation_course.payscale %></td>
                            <td><%= @orientation_course.program_course %></td>
                            <td><%= @orientation_course.pin %></td>
                            <td><%= @orientation_course.std_code %></td>
                            <td><%= @orientation_course.landline %></td>
                            <td><%= @orientation_course.mobile %></td>
                            <td><%= @orientation_course.university_pin %></td>
                            <td><%= @orientation_course.dob %></td>
                            <td><%= @orientation_course.date_of_joining %></td>
                            <td><%= @orientation_course.pref_date1 %></td>
                            <td><%= @orientation_course.pref_date2 %></td>
                            <td><%= @orientation_course.res_address %></td>
                            <td><%= @orientation_course.phd %></td>
                        </tbody>    
                      </table>
                </div>
                <!-- /.table-responsive -->
            <%= link_to 'Edit', edit_orientation_course_path(@orientation_course), :class => 'btn btn-default' %> 
            <%= link_to 'Destroy', orientation_course_path, :class => 'btn btn-default', method: :delete, data: { confirm: 'Are you sure?' } %>
            <%= link_to 'Back', orientation_record_path, :class => 'btn btn-default' %>        
        </div>
        <!-- /.col-lg-12 -->
</div>
<!-- /. row -->